# Environment Configuration
NODE_ENV=development

# Logging Level (debug, info, warn, error)
LOG_LEVEL=info

# Git provider (GitHub, GitLab, etc.)
GIT_PROVIDER=github
GIT_REPO_URL=owner/repository
GIT_ACCESS_TOKEN=ghp_your_github_personal_access_token

# CI/CD Integration (uses GIT_ACCESS_TOKEN if not specified)
CI_API_BASE_URL=https://api.github.com
CI_API_TOKEN=

# Discovery System Integration
METRICS_API_URL=http://localhost:5000
METRICS_API_KEY=
DISCOVERY_URL=http://localhost:5000

# Web Research
RESEARCH_API_URL=https://api.search.example.com
RESEARCH_API_KEY=
WEB_RESEARCH_API_KEY=
WEB_RESEARCH_ENGINE=duckduckgo

# OpenAI or LLM Provider
OPENAI_API_KEY=sk-...

# Anthropic (backup LLM provider)

# Database
DB_URL=postgres://user:pass@localhost:5432/agi_proto

# AIE Cycle (seconds)
AIE_CYCLE_INTERVAL_SECONDS=600

# PAK Long-Horizon (hours)
PAK_LONG_HORIZON_INTERVAL_HOURS=24
